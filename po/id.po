#: src/as-validator.c:1328 src/as-validator.c:1793 src/as-validator.c:1813
#: src/as-validator.c:1846 src/as-validator.c:1889
#, c-format
msgid "Found: %s - Allowed: %s"
msgstr "Ditemukan: %s - Diizinkan: %s"

